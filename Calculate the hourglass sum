#include<bits/stdc++.h>
using namespace std;

int main(){
    vector<int> v1;
    int arr[6][6];

    for (int i = 0; i < 6; i++)                 
{
    for (int j = 0; j < 6; j++)
    {
        cin >> arr[i][j]; 
    }   
}
    
    for(int i=1;i<5;i++){
        for(int j=1;j<5;j++){
            //cout<<arr[i][j]<<endl;
            int sum=(arr[i-1][j-1]+arr[i-1][j]+arr[i-1][j+1]+arr[i][j]+arr[i+1][j-1]+arr[i+1][j]+arr[i+1][j+1]);
            v1.push_back(sum);
        }
        }
    int max=v1[0];
    for(int i=0;i<v1.size();i++){
        if(max<v1[i]){
            max=v1[i];
        }
    }
    cout<<max;
    
}
